{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "framework": null,
  "rootDirectory": "./",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://mentorsuites-backend.vercel.app/api/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
