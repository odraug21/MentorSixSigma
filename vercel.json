{
  "version": 2,
  "outputDirectory": "frontend/build",
  "buildCommand": "cd frontend && npm install && npm run build",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://mentorsuites-backend.vercel.app/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "frontend/build/index.html" }
  ]
}
